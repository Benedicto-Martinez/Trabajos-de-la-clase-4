using System;

class Program
{
    static void Main()
    {
        int numero = 1;

        while (true)
        {
            if (numero % 7 == 0 && numero % 11 == 0)
            {
                Console.WriteLine($"El primer n√∫mero entero positivo divisible entre 7 y 11 es: {numero}");
                break;
            }

            numero++;
        }
    }
}
